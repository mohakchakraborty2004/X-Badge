generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
  binaryTargets = ["native", "rhel-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Badge {
  NPC
  NGMI 
  MGMI
  YGMI
  WAGMI
}

model user {
  id  String @id @default(uuid())
  profileUrl String?
  ghStars Int
  remarks String
  Trepos Int
  Tcommits Int
  jobLevel String
  Xid String @unique 
  Xusername String 
  followers Int
  Xname String 
  about String
  location String @default("ForkedUp Inc.")
  FullName String
  QrUrl String
  NgmiBadge String @default("NPC")
  Worth    String
  created_At String
}